		<expression>Максимум(СуммаНДСНоменклатурнойГруппы)</expression>
		<group>НоменклатурнаяГруппа</group>
		<group>НоменклатурнаяГруппа Иерархия</group>
	</totalField>
	<parameter>
		<name>НачалоПериода</name>
		<title xsi:type="v8:LocalStringType">
