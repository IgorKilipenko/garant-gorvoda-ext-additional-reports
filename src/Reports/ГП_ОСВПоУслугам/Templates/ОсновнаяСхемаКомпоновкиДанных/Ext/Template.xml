    ЕСТЬNULL(ВТ_Результат.Контрагент, ВТ_ОстаткиИОборотыОбщие.Контрагент) КАК Контрагент,
    ВТ_Результат.Дата КАК Дата,
    ВТ_Результат.Документ КАК Документ,
    ЕСТЬNULL(ВТ_Результат.Договор, ВТ_ОстаткиИОборотыОбщие.Договор) КАК Договор,
    ВТ_Результат.КорСубконто1 КАК КорСубконто1,
    ВТ_Результат.КорСубконто2 КАК КорСубконто2,
    ВТ_Результат.КорСубконто3 КАК КорСубконто3,
    ВТ_Результат.Валюта КАК Валюта,
    ВТ_Результат.ВалютнаяСумма КАК ВалютнаяСумма,
    ВТ_Результат.ВидПрочихДоходовИРасходов КАК ВидПрочихДоходовИРасходов,
    ВТ_Результат.Счет КАК Счет,
    ВТ_Результат.КорСчет КАК КорСчет,
    ЕСТЬNULL(ВТ_Результат.Дебет, 0) КАК Дебет,
    ЕСТЬNULL(ВТ_Результат.Кредит, 0) КАК Кредит,
    ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаНачало, 0) КАК ОстатокНаНачало,
    ВЫБОР
        КОГДА ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаНачало, 0) &gt; 0
            ТОГДА ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаНачало, 0)
        ИНАЧЕ 0
    КОНЕЦ КАК ОстатокНаНачалоДт,
    ВЫБОР
        КОГДА ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаНачало, 0) &lt; 0
            ТОГДА -ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаНачало, 0)
        ИНАЧЕ 0
    КОНЕЦ КАК ОстатокНаНачалоКт,
    ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.Оборот, 0) КАК Оборот,
    ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОборотДт, 0) КАК ОборотДт,
    ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОборотКт, 0) КАК ОборотКт,
    ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаКонец, 0) КАК ОстатокНаКонец,
    ВЫБОР
        КОГДА ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаКонец, 0) &gt; 0
            ТОГДА ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаКонец, 0)
        ИНАЧЕ 0
    КОНЕЦ КАК ОстатокНаКонецДт,
    ВЫБОР
        КОГДА ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаКонец, 0) &lt; 0
            ТОГДА -ЕСТЬNULL(ВТ_ОстаткиИОборотыОбщие.ОстатокНаКонец, 0)
        ИНАЧЕ 0
    КОНЕЦ КАК ОстатокНаКонецКт
ПОМЕСТИТЬ ВТ_Результат_ДляИтогов
ИЗ
    ВТ_Результат КАК ВТ_Результат
        ПОЛНОЕ СОЕДИНЕНИЕ ВТ_ОстаткиИОборотыОбщие КАК ВТ_ОстаткиИОборотыОбщие
        ПО ВТ_Результат.Контрагент = ВТ_ОстаткиИОборотыОбщие.Контрагент
        И ВТ_Результат.Договор = ВТ_ОстаткиИОборотыОбщие.Договор
{ГДЕ
    (ЕСТЬNULL(ВТ_Результат.Договор, ВТ_ОстаткиИОборотыОбщие.Договор)).lc_ВидДоговора В (&amp;ДополнительныеВидыДоговоров)
}
;

