		<query>////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЛицевыеСчета.ЛицевойСчет КАК ЛицевойСчет,
	ЛицевыеСчета.Действует КАК Действует,
	ОтветственныйСобственникЛицевогоСчета.ОтветственныйВладелец КАК КонтрагентЛС
ПОМЕСТИТЬ ВТ_ЛицевыеСчета_Подготовка
ИЗ
	РегистрСведений.КВП_ЛицевыеСчета.СрезПоследних(&amp;Период, ) КАК ЛицевыеСчета
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.УПЖКХ_ОтветственныйСобственникНанимательЛицевогоСчета.СрезПоследних(&amp;Период, ) КАК ОтветственныйСобственникЛицевогоСчета
		ПО (ОтветственныйСобственникЛицевогоСчета.ОтветственныйВладелец ССЫЛКА Справочник.Контрагенты)
			И ЛицевыеСчета.ЛицевойСчет = ОтветственныйСобственникЛицевогоСчета.ОтветственныйВладелец
ГДЕ
	ВЫБОР
			КОГДА &amp;ТолькоДействующиеЛицевыеСчета = ИСТИНА
				ТОГДА ЛицевыеСчета.Действует = ИСТИНА
			ИНАЧЕ ИСТИНА
		КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ_ЛицевыеСчета_Подготовка.ЛицевойСчет КАК ЛицевойСчет,
	ВТ_ЛицевыеСчета_Подготовка.Действует КАК Действует,
	ВТ_ЛицевыеСчета_Подготовка.КонтрагентЛС КАК КонтрагентЛС,
	МАКСИМУМ(ДоговорыКонтрагентов.Ссылка) КАК ДоговорЛС
ПОМЕСТИТЬ ВТ_ЛицевыеСчета
ИЗ
	ВТ_ЛицевыеСчета_Подготовка КАК ВТ_ЛицевыеСчета_Подготовка
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ДоговорыКонтрагентов КАК ДоговорыКонтрагентов
		ПО (ДоговорыКонтрагентов.ЭтоГруппа = ЛОЖЬ)
			И (ДоговорыКонтрагентов.ПометкаУдаления = ЛОЖЬ)
			И (НЕ ВТ_ЛицевыеСчета_Подготовка.КонтрагентЛС ЕСТЬ NULL)
			И (ДоговорыКонтрагентов.lc_ВидДоговора В (&amp;ВидДоговораАбонентскийОтдел))
			И (ДоговорыКонтрагентов.Владелец = ВТ_ЛицевыеСчета_Подготовка.КонтрагентЛС)

СГРУППИРОВАТЬ ПО
	ВТ_ЛицевыеСчета_Подготовка.ЛицевойСчет,
	ВТ_ЛицевыеСчета_Подготовка.Действует,
	ВТ_ЛицевыеСчета_Подготовка.КонтрагентЛС
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ_ЛицевыеСчета.ЛицевойСчет КАК ЛицевойСчет,
	ЕСТЬNULL(СведенияДляВзаиморасчетовПоЛСАбонентов.Договор, ЕСТЬNULL(ВТ_ЛицевыеСчета.ДоговорЛС, ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка))) КАК Договор,
	ЕСТЬNULL(СведенияДляВзаиморасчетовПоЛСАбонентов.Контрагент, ЕСТЬNULL(ВТ_ЛицевыеСчета.КонтрагентЛС, ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка))) КАК Контрагент
ПОМЕСТИТЬ ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов
ИЗ
	ВТ_ЛицевыеСчета КАК ВТ_ЛицевыеСчета
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.УПЖКХ_СведенияДляВзаиморасчетовПоЛС.СрезПоследних(&amp;Период, Организация = &amp;Организация) КАК СведенияДляВзаиморасчетовПоЛСАбонентов
		ПО ВТ_ЛицевыеСчета.ЛицевойСчет = СведенияДляВзаиморасчетовПоЛСАбонентов.ЛицевойСчет
ГДЕ
	СведенияДляВзаиморасчетовПоЛСАбонентов.Договор.lc_ВидДоговора В(&amp;ВидДоговораАбонентскийОтдел)
	И ВЫБОР
			КОГДА &amp;ТолькоДействующиеЛицевыеСчета = ИСТИНА
				ТОГДА ВТ_ЛицевыеСчета.Действует = ИСТИНА
			ИНАЧЕ ИСТИНА
		КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДинамическиеПоказателиЛицевыхСчетов.ЛицевойСчет КАК ЛицевойСчет,
	ДинамическиеПоказателиЛицевыхСчетов.ВидПотребления КАК ВидПотребления,
	ДинамическиеПоказателиЛицевыхСчетов.Финансирование КАК СпособФинансирования,
	ДинамическиеПоказателиЛицевыхСчетов.Водозабор КАК Водозабор,
	ДинамическиеПоказателиЛицевыхСчетов.Договородержатель КАК Договородержатель,
	ДинамическиеПоказателиЛицевыхСчетов.Контролер КАК Контролер,
	ДинамическиеПоказателиЛицевыхСчетов.Характеристика КАК Характеристика
ПОМЕСТИТЬ ВТ_ДинамическиеПоказателиЛицевыхСчетов
ИЗ
	РегистрСведений.lc_ДинамическиеПоказателиЛицевыхСчетов.СрезПоследних(&amp;Период, ) КАК ДинамическиеПоказателиЛицевыхСчетов
	//### ВТ_ДинамическиеПоказателиЛицевыхСчетов КАК ДинамическиеПоказателиЛицевыхСчетов //### 
ГДЕ
	ДинамическиеПоказателиЛицевыхСчетов.ЛицевойСчет В
			(ВЫБРАТЬ РАЗЛИЧНЫЕ
				Т.ЛицевойСчет
			ИЗ
				ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов КАК Т)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВидыТарифовПотребителейЛицевыхСчетов.ЛицевойСчет КАК ЛицевойСчет,
	ВидыТарифовПотребителейЛицевыхСчетов.ВидТарифаПотребителя КАК ВидТарифаПотребителя
ПОМЕСТИТЬ ВТ_ВидыТарифовПотребителейЛицевыхСчетов
ИЗ
	РегистрСведений.ГП_ВидыТарифовПотребителейЛицевыхСчетов.СрезПоследних(&amp;Период, ) КАК ВидыТарифовПотребителейЛицевыхСчетов
	//### ВТ_ВидыТарифовПотребителейЛицевыхСчетов КАК ВидыТарифовПотребителейЛицевыхСчетов //### 
ГДЕ
	ВидыТарифовПотребителейЛицевыхСчетов.ЛицевойСчет В
			(ВЫБРАТЬ РАЗЛИЧНЫЕ
				Т.ЛицевойСчет
			ИЗ
				ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов КАК Т)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.Договор КАК Договор,
	ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.Договор.Номер КАК НомерДоговора,
	ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.Контрагент КАК Контрагент,
	ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.ЛицевойСчет КАК ЛицевойСчет,
	ВТ_ДинамическиеПоказателиЛицевыхСчетов.ВидПотребления КАК ВидПотребления,
	ВТ_ДинамическиеПоказателиЛицевыхСчетов.СпособФинансирования КАК СпособФинансирования,
	ВТ_ДинамическиеПоказателиЛицевыхСчетов.Водозабор КАК Водозабор,
	ВТ_ДинамическиеПоказателиЛицевыхСчетов.Договородержатель КАК Договородержатель,
	ВТ_ДинамическиеПоказателиЛицевыхСчетов.Контролер КАК Контролер,
	ВТ_ДинамическиеПоказателиЛицевыхСчетов.Характеристика КАК Характеристика,
	ВТ_ВидыТарифовПотребителейЛицевыхСчетов.ВидТарифаПотребителя КАК ВидТарифаПотребителя
ИЗ
	ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов КАК ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ДинамическиеПоказателиЛицевыхСчетов КАК ВТ_ДинамическиеПоказателиЛицевыхСчетов
		ПО (ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.ЛицевойСчет = ВТ_ДинамическиеПоказателиЛицевыхСчетов.ЛицевойСчет)
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ВидыТарифовПотребителейЛицевыхСчетов КАК ВТ_ВидыТарифовПотребителейЛицевыхСчетов
		ПО (ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.ЛицевойСчет = ВТ_ВидыТарифовПотребителейЛицевыхСчетов.ЛицевойСчет)
{ГДЕ
	(ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.Договор = &amp;Договор),
	(ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.Контрагент = &amp;Контрагент),
	(ЕСТЬNULL(ВТ_СведенияДляВзаиморасчетовПоЛСАбонентов.ЛицевойСчет, ЗНАЧЕНИЕ(Справочник.КВП_ЛицевыеСчета.ПустаяСсылка)) = &amp;ЛицевойСчет),
	(ЕСТЬNULL(ВТ_ДинамическиеПоказателиЛицевыхСчетов.ВидПотребления, ЗНАЧЕНИЕ(Справочник.lc_ГруппыПотребителей.ПустаяСсылка)) = &amp;ВидПотребления),
	(ЕСТЬNULL(ВТ_ДинамическиеПоказателиЛицевыхСчетов.СпособФинансирования, ЗНАЧЕНИЕ(Справочник.lc_СпособыФинансирования.ПустаяСсылка)) = &amp;СпособФинансирования),
	(ЕСТЬNULL(ВТ_ДинамическиеПоказателиЛицевыхСчетов.Водозабор, ЗНАЧЕНИЕ(Справочник.lc_Водозаборы.ПустаяСсылка)) = &amp;Водозабор),
	(ЕСТЬNULL(ВТ_ДинамическиеПоказателиЛицевыхСчетов.Договородержатель, ЗНАЧЕНИЕ(Справочник.lc_Контролеры.ПустаяСсылка)) = &amp;Договородержатель),
	(ЕСТЬNULL(ВТ_ДинамическиеПоказателиЛицевыхСчетов.Контролер, ЗНАЧЕНИЕ(Справочник.lc_Контролеры.ПустаяСсылка)) = &amp;Контролер),
	(ЕСТЬNULL(ВТ_ДинамическиеПоказателиЛицевыхСчетов.Характеристика, ЗНАЧЕНИЕ(Справочник.lc_ХарактеристикиОбъекта.ПустаяСсылка)) = &amp;Характеристика),
	(ЕСТЬNULL(ВТ_ВидыТарифовПотребителейЛицевыхСчетов.ВидТарифаПотребителя, ЗНАЧЕНИЕ(Справочник.lc_ВидыТарифов.ПустаяСсылка)) = &amp;ВидТарифаПотребителя)}
</query>
	</dataSet>
